{
  "name": "M2X PHP Demo App",
  "description": "AT&amp;T M2X Heroku Demo Application",
  "repository": "https://github.com/attm2x/m2x-demo-heroku-php",
  "keywords": ["php", "m2x"],
  "addons": ["m2x"],
  "env": {
    "THRIFTY": {
      "description": "run on a single (free) dyno",
      "value": "true"
    },
    "CONCURRENCY": {
      "description": "level of total concurrency",
      "value": "1"
    },
    "CLOCK_CONCURRENCY": {
      "description": "level of concurrency for clock processes",
      "value": "1"
    }
  }
}
